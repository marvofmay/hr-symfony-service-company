{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
  "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-migrations-bundle": "^3.5.0",
        "doctrine/orm": "3.6.x-dev",
        "dompdf/dompdf": "^3.1",
        "fakerphp/faker": "^1.24.1",
        "gedmo/doctrine-extensions": "^3.21",
        "lexik/jwt-authentication-bundle": "^3.1",
        "nelmio/api-doc-bundle": "^4.38.2",
        "nelmio/cors-bundle": "^2.6",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpoffice/phpspreadsheet": "^3.10.1",
        "phpstan/phpdoc-parser": "^2.3",
        "psr/log": "^3.0.2",
        "ramsey/uuid-doctrine": "^2.1",
        "symfony/amqp-messenger": "7.3.*",
        "symfony/asset": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2.8.2",
        "symfony/framework-bundle": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mercure": "^0.6.5",
        "symfony/mercure-bundle": "^0.3.9",
        "symfony/messenger": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bridge": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/yaml": "7.3.*"
    },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*"
    }
  },
  "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.4.x-dev",
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpstan": "^2.1.31",
        "phpunit/phpunit": "^9.6.29",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.3.4"
    }
}
